<%= simple_form_for(@move) do |f| %>
    <%= f.error_notification %>
    <div class="row">
        <div class="form-group col-lg-5">
            <p>
            </p>
        </div>
    </div>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.association :type %>
  </div>

<p>
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
</p>


<% end %>
